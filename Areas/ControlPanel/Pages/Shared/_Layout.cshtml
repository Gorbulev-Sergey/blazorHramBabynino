<!DOCTYPE html>
<html lang="ru">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="yandex-verification" content="36bacd69ee29c13d" /> @*Для Яндекс поисковика*@
    <meta name="google-site-verification" content="kbCLyv5tdgvDxUtwXDmjoVrjwTU9L3n5iPjYyeRGPX4" /> @*Для Гугл поисковика*@
    <meta name="keywords" content="храм бабынино Вознесения Господня поселок калуга калужская область бабынинский район Вознесение Господне" />
    <meta name="keywords" content="храм бабынино, храм Вознесения Господня, храм поселок бабынино, калуга храм, калужская область храм, бабынинский район храм, Вознесение Господне" />
    <meta name="description" content="Сайт храма Вознесения Господня пос. Бабынино Калужской обл." />
    <title>@ViewData["Title"] - Храм Вознесения Господня</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.9.0/css/all.css">
    <link rel="stylesheet" href="~/css/site.css" />
    <base href="~/" />
</head>
<body>
    <div class="my_sidebar bg-dark noprint">
        <div class="navbar navbar-expand-md bg-dark flex-column align-items-stretch" style="max-width:300px">
            <div class="navbar-brand text-white w-100 pt-1 pl-md-3" style="margin-top:-1px;">
                <button class="navbar-toggler text-white" data-toggle="collapse" data-target="#c_me"><i class="fas fa-bars"></i></button>
                <a class="text-white text-decoration-none" href="/">
                    <h5 class="d-inline-block mt-0 mb-0"><b>Панель управления</b></h5>
                </a>
            </div>
            <nav id="c_me" class="navbar-collapse collapse flex-column align-items-stretch bg-dark text-white rounded w-100 mt-3 mb-2">
                @*Главная*@
                <a class="btn btn-dark btn-block text-left m-0" href="/"><i class="fas fa-home pr-2 ico"></i>Главная</a>
                @*Пользователи*@
                <a class="btn btn-dark btn-block text-left m-0" href="/users"><i class="fas fa-users pr-2 ico"></i>Пользователи</a>
                @*Публикации*@
                <div class="btn-group btn-block rounded p-0 m-0">
                    <a class="btn btn-dark btn-block text-left m-0" href="/posts"><i class="fas fa-newspaper pr-2 ico"></i>Публикации</a>
                    <a class="btn btn-dark" href="/posts/create"><i class="fas fa-plus"></i></a>
                </div>
                @*Комментарии*@
                <a class="btn btn-dark btn-block text-left m-0" href="/comments"><i class="far fa-comments pr-2 ico"></i>Комментарии</a>
                @*Лайки*@
                <a class="btn btn-dark btn-block text-left m-0" href="/likes"><i class="fas fa-heart pr-2 ico"></i>Лайки</a>
                @*Теги*@
                <a class="btn btn-dark btn-block text-left m-0" href="/tags"><i class="fas fa-tags pr-2"></i>Теги</a>
                @*Фотографии*@
                <div class="btn-group btn-block rounded p-0 m-0">
                    <a class="btn btn-dark btn-block text-left m-0" href="/photos"><i class="fas fa-images pr-2 ico"></i>Фотографии</a>
                    <a class="btn btn-dark" href="/photos/create"><i class="fas fa-plus"></i></a>
                </div>
                @*Расписание*@
                <div class="btn-group btn-block rounded p-0 m-0">
                    <a class="btn btn-dark btn-block text-left m-0" href="/schedule"><i class="fas fa-calendar-alt pr-2 ico"></i>Расписание</a>
                    <a class="btn btn-dark" href="/schedule/create"><i class="fas fa-plus"></i></a>
                </div>

                <div hidden class="rounded">
                    @*Публикации*@
                    <div class="btn-group btn-block rounded">
                        <a class="btn btn-dark btn-block text-left m-0" href="/posts"><i class="fas fa-newspaper pr-2 ico"></i>Публикации</a>
                        <button class="btn btn-dark" data-toggle="collapse" data-target="#c1"><span class="fas fa-chevron-down"></span></button>
                    </div>
                    <div class="collapse rounded" id="c1">
                        <a class="btn btn-dark btn-block text-left m-0 pl-4" href="/posts">Все публикации</a>
                        <a class="btn btn-dark btn-block text-left m-0 pl-4" asp-area="Control" asp-controller="posts" asp-action="Create">Создать</a>
                    </div>
                </div>
                <div hidden class="rounded">
                    @*Фотоальбомы*@
                    <div class="btn-group btn-block rounded">
                        <a class="btn btn-dark btn-block text-left" asp-area="Control" asp-controller="imageAlbums" asp-action="Index"><i class="fas fa-images pr-2 ico"></i>Фотоальбомы</a>
                        <button class="btn btn-dark" data-toggle="collapse" data-target="#c2"><i class="fas fa-chevron-down"></i></button>
                    </div>
                    <div class="collapse rounded" id="c2">
                        <a class="btn btn-dark btn-block text-left m-0 pl-4" asp-area="Control" asp-controller="imageAlbums" asp-action="Index">Все фотоальбомы</a>
                        <a class="btn btn-dark btn-block text-left m-0 pl-4" asp-area="Control" asp-controller="imageAlbums" asp-action="Create">Создать</a>
                    </div>
                </div>

            </nav>
        </div>
        <div class="my_header pl-4 pr-2 pt-1 pb-1 pt-md-2 pb-md-2 bg-secondary text-white d-flex justify-content-between align-items-center">
            <h5 class="mb-0 ml-md-2">@ViewBag.Title</h5>
            <nav>
                <a class="btn btn-secondary m-0" href="http://www.hram-babynino.somee.com/" title="Сайт">Сайт</a>
                <a class="btn btn-secondary m-0" href="Identity/Account/Manage/Index" title="Профиль"><h5 class="fas fa-user-circle mb-0"></h5></a>
            </nav>
        </div>
    </div>

    <div class="my_content pt-4 pb-4">
        <div class="container">
            @RenderBody()
        </div>
    </div>





